#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zhangjinhui/Desktop/eee/eee-monorepo/node_modules/.pnpm/vitepress@1.0.0-rc.31_@algolia+client-search@5.18.0_@types+node@20.17.10_postcss@8.4.49_searc_tyrdsbi5phinamgkpuigsgsd2y/node_modules/vitepress/bin/node_modules:/Users/zhangjinhui/Desktop/eee/eee-monorepo/node_modules/.pnpm/vitepress@1.0.0-rc.31_@algolia+client-search@5.18.0_@types+node@20.17.10_postcss@8.4.49_searc_tyrdsbi5phinamgkpuigsgsd2y/node_modules/vitepress/node_modules:/Users/zhangjinhui/Desktop/eee/eee-monorepo/node_modules/.pnpm/vitepress@1.0.0-rc.31_@algolia+client-search@5.18.0_@types+node@20.17.10_postcss@8.4.49_searc_tyrdsbi5phinamgkpuigsgsd2y/node_modules:/Users/zhangjinhui/Desktop/eee/eee-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zhangjinhui/Desktop/eee/eee-monorepo/node_modules/.pnpm/vitepress@1.0.0-rc.31_@algolia+client-search@5.18.0_@types+node@20.17.10_postcss@8.4.49_searc_tyrdsbi5phinamgkpuigsgsd2y/node_modules/vitepress/bin/node_modules:/Users/zhangjinhui/Desktop/eee/eee-monorepo/node_modules/.pnpm/vitepress@1.0.0-rc.31_@algolia+client-search@5.18.0_@types+node@20.17.10_postcss@8.4.49_searc_tyrdsbi5phinamgkpuigsgsd2y/node_modules/vitepress/node_modules:/Users/zhangjinhui/Desktop/eee/eee-monorepo/node_modules/.pnpm/vitepress@1.0.0-rc.31_@algolia+client-search@5.18.0_@types+node@20.17.10_postcss@8.4.49_searc_tyrdsbi5phinamgkpuigsgsd2y/node_modules:/Users/zhangjinhui/Desktop/eee/eee-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
