#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zhangjinhui/Desktop/eee/eee-monorepo/node_modules/.pnpm/vite@6.0.3_@types+node@20.17.10_jiti@1.21.7_yaml@2.6.1/node_modules/vite/bin/node_modules:/Users/zhangjinhui/Desktop/eee/eee-monorepo/node_modules/.pnpm/vite@6.0.3_@types+node@20.17.10_jiti@1.21.7_yaml@2.6.1/node_modules/vite/node_modules:/Users/zhangjinhui/Desktop/eee/eee-monorepo/node_modules/.pnpm/vite@6.0.3_@types+node@20.17.10_jiti@1.21.7_yaml@2.6.1/node_modules:/Users/zhangjinhui/Desktop/eee/eee-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zhangjinhui/Desktop/eee/eee-monorepo/node_modules/.pnpm/vite@6.0.3_@types+node@20.17.10_jiti@1.21.7_yaml@2.6.1/node_modules/vite/bin/node_modules:/Users/zhangjinhui/Desktop/eee/eee-monorepo/node_modules/.pnpm/vite@6.0.3_@types+node@20.17.10_jiti@1.21.7_yaml@2.6.1/node_modules/vite/node_modules:/Users/zhangjinhui/Desktop/eee/eee-monorepo/node_modules/.pnpm/vite@6.0.3_@types+node@20.17.10_jiti@1.21.7_yaml@2.6.1/node_modules:/Users/zhangjinhui/Desktop/eee/eee-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
